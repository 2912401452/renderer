{
  "name": "renderer",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "yarn run docs:build && yarn run docs:deploy",
    "release": "yarn run build && yarn publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "README.md"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "unpkg": "dist/renderer.js",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "eventemitter3": "^4.0.0",
    "lodash": "^4.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@types/lodash": "^4.14.170",
    "@umijs/test": "^3.0.5",
    "antd": "^4.6.6",
    "dumi": "^1.0.9",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "loadsh": "^0.0.4",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
